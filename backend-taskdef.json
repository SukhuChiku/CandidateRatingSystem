{
  "family": "backend-taskdef",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "environment": [
    { "name": "ASPNETCORE_ENVIRONMENT", "value": "Production" }
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::820477575711:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::820477575711:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "candidateratingbackend",
      "image": "REPLACE_ME",
      "essential": true,
      "portMappings": [
        { "containerPort": 8080}
      ]
    }
  ]
}
