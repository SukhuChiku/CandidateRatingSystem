{
 
  "family": "frontend-taskdef",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
   "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "executionRoleArn": "arn:aws:iam::820477575711:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::820477575711:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "candidateratingfrontendcontainer",
      "image": "REPLACE_ME",
      "essential": true,
      "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/frontend-taskdef",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-west-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
      "portMappings": [
        { 
        "appProtocol": "http",
        "containerPort": 80,
        "hostPort": 80,
        "name": "candidateratingfrontendcontainer-80-tcp",
        "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "REACT_APP_API_BASE_URL",
          "value": "http://candidatebackend-1759328448.us-west-1.elb.amazonaws.com/"
        }
      ]
    }
  ]
}
